





<map name ="Feature Geometry Demo" type ="geocentric" version = "2" >
	<image name = "world" driver="gdal" >
		<url>data/world.tif</url>
	</image>
	
	<feature_model name="states" >
	
	<!-- Configurate the OGR feature driver to read the shapefile -->
	<features name = "states" driver="ogr">
		<url>I:/cp/osgEarth/osgearth/data/usa.shp</url>
	</features>
	
	<styles>
		<style type="text/css">
		states {
		stroke:	#ffff00;
		altitude-offset:	1000;
		render-lighting:	false;
		}
		</style>
	</styles>
	</feature_model>
</map>